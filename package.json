{
  "name": "kokoanlp",
  "version": "0.1.0",
  "description": "Korean Kernel Object Analyzer",
  "main": "lib/index.js",
  "keywords": [
    "korean",
    "alphabet",
    "hangul",
    "analyzer"
  ],
  "directories": {
    "bin": "bin",
    "lib": "lib",
    "test": "test"
  },
  "dependencies": {
    "hanguler": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "parcel": "^1.8.1"
  },
  "scripts": {
    "docs": "npm run docs:api & npm run docs:demo",
    "docs:api": "jsdoc lib -d docs",
    "docs:demo": "parcel build demo/index.html -d docs --out-file demo.html",
    "lint": "eslint lib test",
    "lint:fix": "eslint lib test --fix",
    "test": "node test",
    "train": "node bin/train.js"
  },
  "repository": {
    "type": "git",
